<table class="table" id="tabla-listado-pedidos-historico">
    <thead class="card-table_header">
        <tr>
            <th scope="col" class="border-radius-left" style="padding:13px;">Nombre del Cliente</th>
            <th scope="col" style="padding:13px;">Fecha Creación</th>
            <th scope="col" style="padding:13px;">Observación</th>
            <th scope="col" style="padding:13px;">Número de Orden</th>
            <th scope="col" style="padding:13px;">Orden SAP</th>
            <th scope="col" style="padding:13px;">Compra KG</th>
            <th scope="col" style="padding:13px;">Compra $</th>
            <th scope="col" style="padding:13px;">Valor NC (Sin Iva)</th>
            <th scope="col" style="padding:13px;">Aprobador Actual</th>
            <th scope="col" style="padding:13px;">Estado</th>
            <th scope="col" class="border-radius-right" style="padding:13px;"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.result)
        {
            <tr>
                <td>@item.nombreCliente</td>
                <td>@item.fechaCreacion</td>
                <td>@item.observacion</td>
                <td>@item.numeroOrden</td>
                <td>@item.ordenSAP</td>
                <td>@item.compraKG</td>
                <td>@item.compraUSD</td>
                <td>@item.valorNCSinIva</td>
                <td>@item.aprobadorActual</td>
                <td>@item.estado</td>
                <td>
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" data-bs-toggle="modal" data-bs-target="#modalPedidoEntregado">
                        <g id="01 align center">
                            <path id="Vector" d="M24 3H4.242L4.2 2.649C4.11405 1.91942 3.76338 1.24673 3.21449 0.758478C2.6656 0.270223 1.95663 0.000341793 1.222 0L0 0V2H1.222C1.46693 2.00003 1.70334 2.08996 1.88637 2.25272C2.06941 2.41547 2.18634 2.63975 2.215 2.883L3.8 16.351C3.88595 17.0806 4.23662 17.7533 4.78551 18.2415C5.3344 18.7298 6.04337 18.9997 6.778 19H20V17H6.778C6.53291 16.9999 6.29638 16.9099 6.11333 16.7469C5.93027 16.5839 5.81343 16.3594 5.785 16.116L5.654 15H21.836L24 3ZM20.164 13H5.419L4.478 5H21.607L20.164 13Z" fill="#01ADEF" />
                            <path id="Vector_2" d="M7 24C8.10456 24 8.99999 23.1046 8.99999 22C8.99999 20.8954 8.10456 20 7 20C5.89543 20 5 20.8954 5 22C5 23.1046 5.89543 24 7 24Z" fill="#01ADEF" />
                            <path id="Vector_3" d="M17 24C18.1046 24 19 23.1046 19 22C19 20.8954 18.1046 20 17 20C15.8954 20 15 20.8954 15 22C15 23.1046 15.8954 24 17 24Z" fill="#01ADEF" />
                        </g>
                    </svg>

                </td>
            </tr>
        }
        
        
    </tbody>
</table>
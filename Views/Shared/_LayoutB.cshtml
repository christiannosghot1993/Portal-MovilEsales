        function openNav() {
            var sideBar = document.getElementById('mySidebar');
            var closeMenuBtn = document.getElementById('btnCloseMenu');
            var openSideBar = document.getElementById('btnOpenSideBar');
            sideBar.style.width = '94%';
            sideBar.style.height = '92%';
            sideBar.style.marginLeft = '15px';
            sideBar.style.marginTop = '60px';
            closeMenuBtn.style.display = 'inherit';
            openSideBar.style.display = 'none';
        }

        function closeNav() {
            var closeMenuBtn = document.getElementById('btnCloseMenu');
            var openSideBar = document.getElementById('btnOpenSideBar');
            var sideBar = document.getElementById('mySidebar');
            var main = document.getElementById('main');
            sideBar.style.width = '0';
            main.style.marginLeft = '0';
            closeMenuBtn.style.display = 'none';
            openSideBar.style.display = 'inherit';
        }
    </script>

